<div class="container">
<p style="color: green"><%= notice %></p>

<h1>Projects</h1>
<%= link_to "New project", new_project_path %>
<hr>
<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
    <p>
      <%= link_to "Show", project %>
    </p>
    <hr>
  <% end %>

</div>
</div>
